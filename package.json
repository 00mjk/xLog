{
  "name": "proselog",
  "private": true,
  "description": "A publishing platform",
  "license": "unlicensed",
  "sideEffects": false,
  "scripts": {
    "build-remix": "remix build",
    "build": "pnpm build-css && pnpm build-remix && pnpm build-vercel",
    "build-vercel": "node -r esbuild-register scripts/vercel-build.ts",
    "dev-node": "nodemon build/index.js --watch build",
    "dev-remix": "remix watch",
    "dev": "pnpm generate-css && run-p dev-*",
    "generate-css": "tailwindcss --input ./css/main.css --output ./app/generated.css --postcss",
    "dev-css": "pnpm generate-css --watch",
    "build-css": "pnpm generate-css --minify"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.81.0",
    "@codemirror/commands": "^0.20.0",
    "@codemirror/history": "^0.19.2",
    "@codemirror/lang-markdown": "^0.20.0",
    "@codemirror/state": "^0.20.0",
    "@codemirror/view": "^0.20.3",
    "@headlessui/react": "^1.6.0",
    "@prisma/client": "^3.13.0",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/vercel": "*",
    "@uiw/react-codemirror": "^4.7.0",
    "@vercel/node": "^1.14.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.1",
    "form-data": "^4.0.0",
    "mailgun.js": "^5.2.2",
    "markdown-it": "^13.0.0",
    "nanoid": "^3.3.3",
    "react": "^17.0.2",
    "react-avatar-editor": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.2.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "zod": "^3.14.4",
    "zustand": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@remix-run/serve": "*",
    "@types/connect": "^3.4.35",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^17.0.24",
    "@types/react-avatar-editor": "^12.0.0",
    "@types/react-dom": "^17.0.9",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.11.0",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.1.0",
    "prisma": "^3.13.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
